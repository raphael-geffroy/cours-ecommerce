{% extends "base.html.twig" %}
{% block title %}
	Modification d'une catégorie
{% endblock %}
{% block body %}
	<h1>Catégorie</h1>

	{{ form_start(formView) }}
	{{ form_errors(formView) }}
	{#{{ form_widget(formView) }}#}
	<div class="row">
		<div class="col">
			{{ form_row(formView.name) }}
		</div>
	</div>


	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Modifier la catégorie
	</button>
	{{ form_end(formView) }}
{% endblock %}
